  },
  "devDependencies": {
    "@types/dotenv-defaults": "2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.2.0",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0"
  }
}
